    "redux": "^3.1.2",
    "redux-logger": "^2.6.1",
    "redux-multi": "^0.1.12",
    "redux-promise-middleware": "^4.0.0"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "expect": "^1.20.2",
