{
  "name": "indoqa-react-redux",
  "version": "2.1.0",
  "description": "Indoqa React/Redux Archetype",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Indoqa/indoqa-react-redux.git"
  },
  "scripts": {
    "start": "indoqa-dev-server ./indoqa-webpack-options.js",
    "build": "indoqa-webpack ./indoqa-webpack-options.js",
    "docs": "indoqa-webpack ./indoqa-webpack-docs.js",
    "flow": "flow",
    "lint": "indoqa-eslint ./src/main",
    "package": "yarn run lint && yarn run flow && yarn run test && indoqa-webpack ./indoqa-webpack-options.js",
    "test": "indoqa-jest",
    "test:watch": "yarn run test -- --watch",
    "postinstall": "yarn run package"
  },
  "dependencies": {
    "fela": "6.0.5",
    "fela-monolithic": "5.0.13",
    "fela-plugin-named-media-query": "5.0.13",
    "fela-preset-web": "7.0.2",
    "indoqa-react-app": "2.4.6",
    "indoqa-react-fela": "0.4.4",
    "prop-types": "15.6.0",
    "ramda": "0.25.0",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-fela": "6.1.0",
    "react-redux": "5.0.6",
    "react-router": "3.2.0",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-observable": "0.17.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.2"
  },
  "devDependencies": {
    "flow-bin": "0.47.0",
    "indoqa-webpack": "1.0.5"
  },
  "proxy": {
    "/geonames": {
      "target": "http://api.geonames.org",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "pathRewrite": {
        "/geonames": ""
      }
    }
  }
}
